{
  "manifest_version": 3,
  "name": "Purpose Mode",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "ui.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  }
}
